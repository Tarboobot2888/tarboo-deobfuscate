{
  "version": 2,
  "builds": [
    {
      "src": "api/deobfuscate.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/", "dest": "/public/index.html" },
    { "src": "/api/(.*)", "dest": "/api/$1.js" }
  ]
}
